#include "stm32l4xx.h"
#define RCC_BASEADDR 0x40021000U
#define RCC_AHB2ENR (*(volatile uint32_t*)(RCC_BASEADDR + 0x4C))

int main(void) {
    // Enable GPIOB clock
    RCC_AHB2ENR |= (1 << 1);
    // Check if the bit is set correctly by viewing in debug mode
    while (1);
}
